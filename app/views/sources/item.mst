<tr>
    <td title="ID">{{id}}</td>
    <td title="Название">{{name}}</td>
    <td class="actions">
        <a href="/sources/edit/{{id}}" class="mdi mdi-database-edit-outline" title="Редактировать"></a>
        <a class="mdi mdi-delete" title="Удалить" onclick="___mc.events.push('sources.remove', {id: {{id}}, name:'{{name}}' })"></a>
